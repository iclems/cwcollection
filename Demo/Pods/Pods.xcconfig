FRAMEWORK_SEARCH_PATHS = "$(PODS_ROOT)/Firebase"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/Block-KVO" "${PODS_ROOT}/Headers/DTCoreText" "${PODS_ROOT}/Headers/DTFoundation" "${PODS_ROOT}/Headers/Firebase" "$(SDKROOT)/usr/include/libxml2"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers" -isystem "${PODS_ROOT}/Headers/Block-KVO" -isystem "${PODS_ROOT}/Headers/DTCoreText" -isystem "${PODS_ROOT}/Headers/DTFoundation" -isystem "${PODS_ROOT}/Headers/Firebase"
OTHER_LDFLAGS = -ObjC -lc++ -licucore -lxml2 -framework CFNetwork -framework CoreGraphics -framework CoreText -framework Firebase -framework ImageIO -framework MediaPlayer -framework QuartzCore -framework Security -framework SystemConfiguration
PODS_ROOT = ${SRCROOT}/Pods